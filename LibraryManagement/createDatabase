CREATE TABLE `Student` (
  `studentId` int(11) NOT NULL,
  `studentName` varchar(45) NOT NULL,
  `studentPass` varchar(45) NOT NULL,
  `studentCourse` varchar(45) NOT NULL,
  `studentGender` int(11) NOT NULL,
  `bookId1` int(11) DEFAULT NULL,
  `bookId2` int(11) DEFAULT NULL,
  PRIMARY KEY (`studentId`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;


CREATE TABLE `Book` (
  `bookId` int(11) NOT NULL,
  `bookName` varchar(45) NOT NULL,
  `Category` varchar(45) DEFAULT NULL,
  `totalBooks` int(11) NOT NULL,
  `availableBooks` int(11) DEFAULT NULL,
  `bookPrize` double NOT NULL,
  PRIMARY KEY (`bookId`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
