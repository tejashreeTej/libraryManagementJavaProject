--I have MySQL on Virtual machine so I used  following Queries --

CREATE USER 'dbuser'@'%' IDENTIFIED BY 'abcd';

GRANT ALL PRIVILEGES ON libraryDatabase.* TO 'dbuser'@'%' WITH GRANT OPTION;

SHOW GRANTS FOR 'dbuser'@'%';


--If MySQL on localhost . Use following Queries --

CREATE USER 'dbuser'@'localhost' IDENTIFIED BY 'abcd';

GRANT ALL PRIVILEGES ON libraryDatabase.* TO 'dbuser'@'localhost' WITH GRANT OPTION;

SHOW GRANTS FOR 'dbuser'@'localhost';
